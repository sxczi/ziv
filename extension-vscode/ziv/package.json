{
    "name": "ziv",
    "icon": "./ziv.png",
    "displayName": "Ziv",
    "description": "Ziv Programming Language",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/uwuxia/ziv.git"
    },
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "iconThemes": [
			{
				"id": "ziv-icons",
				"label": "Ziv Icons",
				"path": "./fileicons/ziv-icon-theme.json",
				"_watch": true
			}
		],
        "languages": [{
            "id": "ziv",
            "aliases": ["Ziv Programming Language", "ziv"],
            "extensions": [".ziv"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ziv",
            "scopeName": "source.ziv",
            "path": "./syntaxes/ziv.tmLanguage.json"
        }]
    }
}